# KaggleSlayer Environment Configuration
# Copy this file to .env and configure your actual values

# === REQUIRED: OpenRouter API Configuration ===
# Get free API key from https://openrouter.ai/
# KaggleSlayer uses 100% free models to maintain $0.00 cost
OPENROUTER_API_KEY=your_openrouter_api_key_here

# === REQUIRED: Kaggle API Configuration ===
# Get credentials from https://www.kaggle.com/settings/account
# Used for downloading competition data and making submissions
KAGGLE_USERNAME=your_kaggle_username
KAGGLE_KEY=your_kaggle_api_key

# === OPTIONAL: Advanced LLM Configuration ===
# OpenAI API Key (only if you want to use paid models - not recommended)
# OPENAI_API_KEY=your_openai_api_key_here

# Hugging Face Token (for some specialized models)
# HUGGINGFACE_TOKEN=your_hf_token_here

# === Pipeline Optimization Settings ===
# Maximum LLM calls per agent (prevents runaway costs)
MAX_LLM_CALLS_PER_AGENT=50

# Enable response caching (recommended for cost savings)
CACHE_LLM_RESPONSES=true

# Debug mode for LLM calls (useful for development)
DEBUG_LLM_CALLS=false

# Default execution mode (autonomous, interactive, or manual)
DEFAULT_MODE=autonomous

# Enable detailed logging for troubleshooting
ENABLE_DETAILED_LOGGING=true